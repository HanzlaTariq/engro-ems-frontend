{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": { "distDir": "dist" }
    }
  ],
  "routes": [
    { "src": "/(.*\\.(js|mjs|css|wasm|json|map|png|jpg|jpeg|gif|svg|ico))$", "dest": "/$1" },
    { "src": "/(.*)", "dest": "/index.html" }
  ]
}

